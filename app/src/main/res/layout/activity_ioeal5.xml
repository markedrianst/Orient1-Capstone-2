            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:paddingBottom="24dp"
            tools:ignore="ExtraText">

            <TextView
                android:text="How to overcome stress in Education"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_bold"
                android:gravity="center"
                android:textColor="#FFFFFF"
                android:textSize="20dp" />

            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true"
                    android:scaleType="centerCrop"
                    android:src="@drawable/v_image1"/>
            </androidx.cardview.widget.CardView>
            <TextView
                android:text="STRESSOR"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_bold"
                android:gravity="center"
                android:layout_marginTop="20dp"
                android:textColor="#FFFFFF"
                android:textSize="20dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_light"
                android:gravity="left"
                android:justificationMode="inter_word"
                android:padding="15dp"
                android:text="A stressor is anything that causes stress or
triggers a response of mental, emotional, or
physical tension."
                android:textColor="#FFFFFF"
                android:textSize="14dp"
                tools:ignore="TextSizeCheck" />

            <TextView
                android:text="STRESS"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_bold"
                android:gravity="center"
                android:layout_marginTop="20dp"

                android:textColor="#FFFFFF"
                android:textSize="20dp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_light"
                android:gravity="left"
                android:justificationMode="inter_word"
                android:padding="15dp"
                android:text="Stress happens when your body and mind feel pressured or overwhelmed, like during a big test or too much work. A little stress can help you focus, but too much for too long can make you feel tired, worried, or even sick."
                android:textColor="#FFFFFF"
                android:textSize="14dp"
                tools:ignore="TextSizeCheck" />

